{
  "name": "enableit-nfs",
  "version": "0.3.0",
  "author": "EnableIT",
  "summary": "NFS Client and Server",
  "license": "Apache-2.0",
  "source": "-",
  "project_page": "-",
  "issues_url": "-",
  "description": "This module installs an NFS client and server and configures the exports. Based on jbeard6-nfs.",
  "data_provider": "hiera",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6.0",
        "7.0",
        "7.1",
        "7.2"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8.0",
        "8.0"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "jbeard/portmap",
      "version_requirement": ">= 0.1.11 < 0.2.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": "2.x"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": "4.x.x"
    }
  ]
}
